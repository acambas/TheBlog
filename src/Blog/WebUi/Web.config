  <connectionStrings>
    <add name="RavenDB" connectionString="Url=http://localhost:8080/databases/theblog" />
    <add name="RavenDBEmbedded" connectionString="DataDir=~\App_Data\Database" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="AdminUsername" value="admin" />
    <add key="AdminPassword" value="123456" />
  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <caching>
      <outputCacheSettings>
        <outputCacheProfiles>
          <add name="Gallery" duration="3600" location="Server" varyByParam="*" />
          <add name="BlogPost" duration="3600" location="Server" varyByParam="*" />
          <add name="BlogPage" duration="3600" location="Server" varyByParam="*" />
          <add name="BlogPageData" duration="3600" varyByParam="*" />
          <add name="Image" duration="1800" varyByParam="id" />
          <add name="StaticPage" duration="3600" />
        </outputCacheProfiles>
      </outputCacheSettings>
    </caching>
  <!-- Glimpse: This can be commented in to add additional data to the Trace tab when using WebForms
        <trace writeToDiagnosticsTrace="true" enabled="true" pageOutput="false"/> --><httpModules>
            <add name="Glimpse" type="Glimpse.AspNet.HttpModule, Glimpse.AspNet" />
        </httpModules><httpHandlers>
            <add path="glimpse.axd" verb="GET" type="Glimpse.AspNet.HttpHandler, Glimpse.AspNet" />
        </httpHandlers></system.web>
  <system.identityModel>
    <identityConfiguration>
      <claimsAuthorizationManager type="WebUi.App_Start.AuthorizationManager, WebUi" />
    </identityConfiguration>
  </system.identityModel>
  <system.webServer>
    <modules>
      <remove name="FormsAuthenticationModule" />
    <add name="Glimpse" type="Glimpse.AspNet.HttpModule, Glimpse.AspNet" preCondition="integratedMode" /></modules>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    <add name="Glimpse" path="glimpse.axd" verb="GET" type="Glimpse.AspNet.HttpHandler, Glimpse.AspNet" preCondition="integratedMode" /></handlers>
  <validation validateIntegratedModeConfiguration="false" /></system.webServer>
